
public class LoopTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int i;
		int j;
		
		for (i=1;i<=5;i++)
			
		{
			System.out.println("*"+" ");
			
			for(j=10;j<i;j--)
			{
			
			System.out.println("*"+" ");
			}
		}
		}
}
