
public class ArrayExample {
	
	public static void main (String args[])
	
	{
		
    int a[]= new int [5];//declaration and instantiation

		int a[0]=10;    //initialization
        int a[1]=20;
        int a[2]=30;
        int a[3]=40;
        int a[4]=50;
        int sum[]= new int[2];
        
        for (i=0;i=a.length;i++)
        	sum[0] = sum + a[i]
        	
        {
	System.out.println(sum);
}
	}
}